{
  "name": "@belloai/chrome-extension-factory",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node ./server",
    "prod": "npm run build && npm run prod:serve",
    "// Build for production": "",
    "build": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "build:clean": "rimraf dist/",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "// Static serve for production": "",
    "prod:serve": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@belloai/chrome-extension-manifest": "workspace:^1.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.19",
    "@types/node-fetch": "^2.5.12",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "3.2.31",
    "@vue/server-renderer": "3.2.31",
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "cross-fetch": "^3.1.5",
    "element-plus": "^2.2.0",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "node-fetch": "^2.6.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-md": "^0.11.8",
    "vite-plugin-ssr": "0.3.64",
    "vite-plugin-style-import": "^2.0.0",
    "vue": "3.2.31"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "engines": {
    "node": "^12 || >=14",
    "pnpm": "^6 || >=5"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^3.1.0",
    "@types/cookie-parser": "^1.4.2",
    "autoprefixer": "^10.4.7",
    "consola": "^2.15.3",
    "postcss": "^8.4.13",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5"
  }
}
